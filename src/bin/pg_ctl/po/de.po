#: pg_ctl.c:1967
#, c-format
msgid "Usage:\n"
msgstr "Aufruf:\n"

#: pg_ctl.c:1968
#, c-format
msgid "  %s init[db]   [-D DATADIR] [-s] [-o OPTIONS]\n"
msgstr "  %s init[db]   [-D DATENVERZ] [-s] [-o OPTIONEN]\n"

